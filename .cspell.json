{
  "dictionaries": [
    "custom-words"
  ],
  "dictionaryDefinitions": [
    {
      "addWords": false,
      "name": "custom-words",
      "path": "./custom-words.txt"
    }
  ],
  "ignorePaths": ["/node_modules/", "/custom-words.txt", "/.git/"],
  "import": [
    "@cspell/dict-en_us/cspell-ext.json",
    "@cspell/dict-de-ch/cspell-ext.json",
    "@cspell/dict-typescript/cspell-ext.json",
    "@cspell/dict-html/cspell-ext.json",
    "@cspell/dict-html-symbol-entities/cspell-ext.json",
    "@cspell/dict-bash/cspell-ext.json",
    "@cspell/dict-git/cspell-ext.json",
    "@cspell/dict-npm/cspell-ext.json",
    "@cspell/dict-node/cspell-ext.json",
    "@cspell/dict-css/cspell-ext.json",
    "@cspell/dict-markdown/cspell-ext.json",
    "@cspell/dict-software-terms/cspell-ext.json",
    "@cspell/dict-public-licenses/cspell-ext.json",
    "@cspell/dict-fullstack/cspell-ext.json",
    "@cspell/dict-companies/cspell-ext.json"
  ],
  "language": "en, de",
  "languageSettings": [
    {
      "caseSensitive": false,
      "languageId": "typescript"
    },
    {
      "caseSensitive": false,
      "languageId": "json"
    },
    {
      "caseSensitive": false,
      "languageId": "javascript"
    }
  ],
  "version": "0.2"
}
